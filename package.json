{
  "name": "express-security",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:client": "webpack --progress --colors -p",
    "build:images": "imagemin ./client/images/* ./public/images/",
    "lint": "eslint . --ext .js,.jade",
    "lint:tests": "eslint ./tests --ext .js",
    "start": "node ./bin/start.js",
    "test": "npm-run-all -s test:*",
    "test:unit": "jasmine-node ./tests/unit --color --verbose --captureExceptions",
    "test:e2e": "jasmine-node ./tests/e2e --color --verbose --captureExceptions",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "nodemon ./bin/start.js",
    "watch:client": "webpack --progress --colors --watch"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "cache-manager": "2.1.2",
    "cache-manager-redis": "0.2.4",
    "compression": "1.6.2",
    "connect-redis": "3.1.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.2.0",
    "express": "4.14.0",
    "express-session": "1.14.1",
    "express-socket.io-session": "1.3.1",
    "extend": "3.0.0",
    "helmet": "2.1.3",
    "jade": "1.11.0",
    "jquery": "3.1.0",
    "moment": "2.14.1",
    "morgan": "1.7.0",
    "ms": "0.7.1",
    "node-uuid": "1.4.7",
    "normalize-scss": "5.0.3",
    "serve-favicon": "2.3.0",
    "socket.io": "1.4.8",
    "socket.io-client": "1.4.8",
    "socket.io-redis": "1.0.0",
    "spdy": "3.4.0",
    "static-asset": "0.6.0",
    "url": "0.11.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.25.0",
    "eslint": "3.5.0",
    "eslint-plugin-jade": "0.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "imagemin-cli": "3.0.0",
    "jasmine-node": "1.14.5",
    "node-sass": "3.9.3",
    "npm-run-all": "3.1.0",
    "postcss-loader": "0.13.0",
    "request": "2.74.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2"
  }
}

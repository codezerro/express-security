{
  "name": "express-security",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "npm run clean && npm-run-all -s build:*",
    "build:client": "cross-env BABEL_ENV=client webpack --progress --colors -p",
    "build:images": "imagemin ./client/images/* ./server/public/images/",
    "build:server": "mkdirp build build/logs build/certificates && cross-env BABEL_ENV=server babel ./server --out-dir ./build --ignore public,views",
    "build:assets": "cp -r ./server/public ./server/config ./server/views ./server/certificates ./build",
    "clean": "npm-run-all -p clean:*",
    "clean:server": "rimraf ./build",
    "clean:client": "rimraf ./server/public/scripts ./server/public/styles",
    "clean:logs": "rimraf ./server/logs ./build/logs",
    "lint": "npm-run-all -s lint:*",
    "lint:client": "eslint ./client --ext .js",
    "lint:server": "eslint ./server --ext .js,.pug",
    "lint:tests": "eslint ./tests --ext .js",
    "start": "npm run build && node ./build/start.js",
    "test": "npm-run-all -s test:*",
    "test:unit": "cross-env BABEL_ENV=server mocha --slow 5 --check-leaks --full-trace --compilers js:babel-register ./tests/unit/",
    "test:e2e": "cross-env BABEL_ENV=server mocha --slow 150 --check-leaks --full-trace --compilers js:babel-register ./tests/e2e/",
    "test:vulnerabilities": "nsp check",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "mkdirp ./server/logs/ && nodemon ./server/start.js",
    "watch:client": "cross-env BABEL_ENV=client webpack --progress --colors --watch"
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "cache-manager": "2.4.0",
    "cache-manager-redis": "0.4.0",
    "compression": "1.6.2",
    "connect-redis": "3.2.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.6.3",
    "express": "4.15.2",
    "express-rate-limit": "2.6.0",
    "express-session": "1.15.1",
    "express-socket.io-session": "1.3.2",
    "extend": "3.0.0",
    "find": "0.2.7",
    "helmet": "3.5.0",
    "http-errors": "1.6.1",
    "jquery": "3.2.1",
    "mime-types": "2.1.15",
    "moment": "2.18.1",
    "morgan": "1.8.1",
    "ms": "1.0.0",
    "node-uuid": "1.4.8",
    "normalize-scss": "6.0.0",
    "parseurl": "1.3.1",
    "pug": "2.0.0-beta11",
    "rate-limit-redis": "1.1.0",
    "serve-favicon": "2.4.1",
    "socket.io": "1.7.3",
    "socket.io-client": "1.7.3",
    "socket.io-redis": "4.0.0",
    "spdy": "3.4.4",
    "static-asset": "0.6.0",
    "url": "0.11.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "brotli-webpack-plugin": "0.2.0",
    "chai": "3.5.0",
    "compression-webpack-plugin": "0.3.2",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "eslint": "3.18.0",
    "eslint-plugin-pug": "1.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "imagemin-cli": "3.0.0",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "node-sass": "4.5.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "request": "2.81.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "set-cookie-parser": "2.0.0",
    "snyk": "1.25.2",
    "style-loader": "0.16.0",
    "supertest": "3.0.0",
    "webpack": "2.3.1"
  }
}

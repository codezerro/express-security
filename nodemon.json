{
  "restartable": "rs",
  "exec": "babel-node",
  "ignore": [
    ".git",
    "build/*",
    "client/*",
    "node_modules/*",
    "server/certificates/*",
    "server/logs/*",
    "server/public/*",
    "tests/*",
    "nodemon.json",
    "package.json"
  ],
  "verbose": true,
  "env": {
    "PORT": 5000,
    "NODE_ENV": "development",
    "BABEL_ENV": "client",
    "DEBUG": "cluster,express:application,socket.io:server,engine:socket,server,config"
  },
  "ext": "js json"
}
